input {
  file {
    type => "symfony-access"
    path => ["path/to/app/prod.access.log"]
  }
}
filter {
  if [type] == "symfony-access" {
    grok {
      match => [ "message", "%{SYMFONY_ACCCESS}" ]
    }
  }
}
output {
  # this will send our event to Elastic Search
  elasticsearch {
    host => "localhost:9200"
    cluster => "elasticsearch"
    node_name => "elastic"
    protocol => http
  }
}

